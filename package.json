{
  "name": "react-tmp",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start":
      "concurrently --names \"React,Rails\" --prefix-colors \"green,blue\" \"react-scripts start\" \"PORT=8080 rails server\"",
    "build":
      "cp public-react/* public && react-scripts build && rm -r public/* && mv build/* public",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
